{
    "description":"apim for HA DMZ with 3 cassandra",
    "containers":[
        {
            "import": ["{compose-root}/tmpl/gwlatestnode.json"],
            "name":"node1",
            "hostname":"node1",
            "gatewayConfig":{
                "IP":"192.168.0.109",
                "node":"node3",
                "ANM":"node1",
                "isANM" : true,
                "port": "8090",
                "groups":[
                    {
                        "name":"MGMT_GROUP",
                        "hasAPIManager":true,
                        "readConsistencyLevel" : "QUORUM",
                        "writeConsistencyLevel" : "QUORUM",
                        "instances":[
                            {
                                "name":"MGMT_01"
                            }
                        ]
                    }
                ]
            },

            "cassandraConfig" : {
            "_comment": "Reserved - do not modify this section.",
                "seedHost":"node1"
            }
        },
        {
            "import": ["{compose-root}/tmpl/gwlatestnode.json"],
            "name":"node2",
            "hostname":"node2",
            "gatewayConfig":{
                "IP":"192.168.0.109",
                "node":"node3",
                "ANM":"node1",
                "isANM" : false,
                "port": "8190",
                "groups":[
                    {
                        "name":"MGMT_GROUP",
                        "hasAPIManager":true,
                        "readConsistencyLevel" : "QUORUM",
                        "writeConsistencyLevel" : "QUORUM",
                        "instances":[
                            {
                                "name":"MGMT_02"
                            }
                        ]
                    }
                ]
            },

            "cassandraConfig" : {
            "_comment": "Reserved - do not modify this section.",
                "seedHost":"node1"
            }
        }
    ]
}
